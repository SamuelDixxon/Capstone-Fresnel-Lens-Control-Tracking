<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginStart="0dp"
    android:layout_marginEnd="0dp"
    android:background="#000000"
    tools:context=".ApplicationInformation">

    <TextView
        android:id="@+id/par2"
        android:layout_width="364dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Title3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/Paragraph2"
        android:textColor="#EF785B"
        android:textSize="16sp"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/B5"
        android:layout_width="91dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="9dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="260dp"
        android:backgroundTint="#F13709"
        android:text="@string/Button5"
        android:textColor="#EF785B"
        android:textColorHint="#FFFFFF"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/Title2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="0dp"
        android:text="@string/Title2"
        android:textAlignment="center"
        android:textColor="#EF785B"
        android:textSize="34sp"
        android:textStyle="bold"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/Title3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/par1"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="-18dp"
        android:layout_marginEnd="32dp"
        android:text="@string/Title3"
        android:textAlignment="center"
        android:textColor="#EF785B"
        android:textSize="34sp"
        android:textStyle="bold"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/par1"
        android:layout_width="366dp"
        android:layout_height="194dp"
        android:layout_below="@+id/Title2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/Paragraph1"
        android:textColor="#EF785B"
        android:textSize="16sp"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="225dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:text="@string/RevHist_1"
        android:textAlignment="textEnd"
        android:textColor="#EF785B"
        android:textStyle="italic"
        tools:ignore="TextContrastCheck" />

</RelativeLayout>